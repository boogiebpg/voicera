<%= form_for meeting do |f| %>
  <div class='form-group'>
    <label for='title'>
      Title
    </label>
    <%= f.text_field :title, class: 'form-control', id: 'title' %>
  </div>
  <div class='form-group'>
    <label for='organizer_email'>
      Organizer Email
    </label>
    <%= f.text_field :organizer_email, class: 'form-control', id: 'organizer_email' %>
  </div>
  <div class='form-group'>
    <label for='start_time'>
      Start Time
    </label>
    <%= f.text_field :start_time, class: 'form-control', id: 'start_time' %>
  </div>
  <div class='form-group'>
    <label for='end_time'>
      End Time
    </label>
    <%= f.text_field :end_time, class: 'form-control', id: 'end_time' %>
  </div>
  <%= f.submit 'Create', class: 'btn btn-primary btn-sm' %>
<% end %>
